# Class-and-object
class student{
    String name;
    int regno;
    String dept;
    String sec;
void combination()
{
    System.out.println(name);
    System.out.println(regno);
    System.out.println(dept);
    System.out.println(sec);
}}
public class Main {
    public static void main(String[] args) {
    student obj = new student();
    obj.name="Aadhu";
    obj.regno=01;
    obj.dept="Ai&Ds";
    obj.sec="A";
    obj.combination();
    }}

  # output
Aadhu
1
Ai&Ds
A
