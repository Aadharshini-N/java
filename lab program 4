class Car {
    String model;
    String color;
    String state;  
    Car(String model, String color) {
        this.model = model;
        this.color = color;
        this.state = ""; 
    }
    void start() {
        state = "started";
        System.out.println("Car started");
    }
    void stop() {
        state = "stopped";
        System.out.println("Car stopped");
    }
}

class Main {
    public static void main(String[] args) {
        // TC1
        Car car1 = new Car("Tesla", "Red");
        car1.start(); 

        // TC2
        Car car2 = new Car("BMW", "Black");
        car2.start(); 

        // TC3 - Empty model
        Car car3 = new Car("", "Blue");
        car3.start();  // Output: Car started (but model not shown)

        // TC4 
        Car car4 = new Car("Audi", "White");
        // No method called, so no output

        // TC5 - Stop only
        Car car5 = new Car("Ford", "Grey");
        car5.stop();   // Output: Car stopped
    }
}


# output
Car started
Car started
Car started
Car stopped

