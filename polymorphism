# polymorphism
class A {
    int a;
    String name, s;
    void set(int a) {
        this.a = a;
        System.out.println("The value of a is " + a);
    }
    void set(int a, String name) {
        this.a = a;
        this.name = name;
        System.out.println("The value of a is " + a + " name is " + name);
    }
    void set(String name, String s) {
        this.name = name;
        this.s = s;
        System.out.println("The value of name is " + name + s);
    }
    public static void main(String[] args) {
        A obj = new A();
        obj.set("aadhu", "shalini"); 
        obj.set(9, "aadharshini");      
        obj.set(8);             
    }

    #output
The value of name is aadhushalini
The value of a is 9 name is aadharshini
The value of a is 8
